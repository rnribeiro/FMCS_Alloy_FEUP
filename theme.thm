<?xml version="1.0"?>
<alloy>

<view>

<defaultnode/>

<defaultedge/>

<node>
   <type name="Car"/>
   <type name="Int"/>
   <type name="String"/>
   <type name="univ"/>
   <type name="seq/Int"/>
   <set name="this/tail" type="Car"/>
</node>

<node color="Gray">
   <set name="this/Unknown" type="VSS"/>
</node>

<node color="Green">
