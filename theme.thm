</node>

<node numberatoms="no">
   <type name="VSS"/>
</node>

<node shape="Trapezoid">
   <set name="this/head" type="Car"/>
</node>

<node style="Bold">
   <set name="this/begin" type="VSS"/>
   <set name="this/end" type="VSS"/>
</node>

<node style="Solid" color="Green">
